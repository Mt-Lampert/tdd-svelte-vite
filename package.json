    "test": "vitest --reporter verbose run src",
    "test:watch": "vitest --reporter verbose src"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.3.0",
    "jsdom": "^20.0.0",
    "svelte": "^3.49.0",
    "vite": "^3.0.0",
    "vitest": "^0.18.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bulma": "^0.9.4",
    "node-sass": "^7.0.1"
  }
}
