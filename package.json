{
  "name": "tdd-svelte-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "css-build": "node-sass --omit-source-map-url src/main.scss src/app.css",
    "css-watch": "npm run css-build -- --watch",
    "dev": "vite",
